# Server Configuration
PORT=3001
NODE_ENV=production

# Google Service Account Credentials
# Get from: https://console.cloud.google.com/iam-admin/serviceaccounts
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour-Private-Key-Here\n-----END PRIVATE KEY-----\n"

# Google Sheets Configuration
GOOGLE_SHEET_ID=your-google-sheet-id-here

# n8n Webhook Configuration
# Since n8n is in the same Docker network, use container name
N8N_WEBHOOK_URL=http://n8n:5678/webhook/your-webhook-path

# API Configuration
CORS_ORIGIN=https://marketing.officepark.online
